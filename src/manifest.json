{
    "name": "Google Meet Chat Flow",
    "description": "nico video like chat flow",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":[
      "tabs",
      "scripting",
      "activeTab",
      "storage"
    ],
    "host_permissions":[
      "https://meet.google.com/*"
    ],
    "action": {
      "default_popup": "./popup/popup.html"
    }
}